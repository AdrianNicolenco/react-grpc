{
  "name": "users-svc",
  "version": "1.0.0",
  "description": "gRPC microservice back-end for users. Used for learning/trial purposes only.",
  "scripts": {
    "start": "nodemon --exec babel-node ./src/main.js",
    "build": "rimraf dist && babel src --out-dir dist --ignore 'tests/**' -s inline --copy-files && cpy package.json dist",
    "test": "jest --watch",
    "lint": "eslint --max-warnings=0 ./src",
    "lint:fix": "eslint --fix ./src",
    "copy:protos": "cpy ../../_proto ./src/_proto",
    "precommit": "npm run lint:fix && npm run lint",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjsicam/node-graphql-microservices.git"
  },
  "keywords": [
    "graphql",
    "microservices",
    "grpc"
  ],
  "author": "Benj Sicam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjsicam/node-graphql-microservices/issues"
  },
  "homepage": "https://github.com/benjsicam/node-graphql-microservices#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@grpc/proto-loader": "^0.5.1",
    "core-js": "^3.2.1",
    "glob": "^7.1.4",
    "grpc": "^1.22.2",
    "ioredis": "^4.14.0",
    "lodash": "^4.17.15",
    "mali": "^0.17.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "pino": "^5.13.2",
    "sequelize": "^5.15.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-inline-dotenv": "^1.3.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cpy-cli": "^2.0.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.1",
    "pino-pretty": "^3.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
}
