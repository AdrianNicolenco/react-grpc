{
  "name": "graphql-gateway",
  "version": "1.0.0",
  "description": "A sample GraphQL API built in Node.js with gRPC back-end microservices. Used for learning/trial purposes only.",
  "scripts": {
    "start": "nodemon --exec babel-node ./src/main.js",
    "build": "rimraf dist && babel src --out-dir dist --ignore 'tests/**' -s inline --copy-files && cpy package.json dist",
    "test": "jest --watch",
    "lint": "eslint --max-warnings=0 ./src",
    "lint:fix": "eslint --fix ./src",
    "copy:protos": "cpy ../_proto ./src/services/_proto",
    "precommit": "npm run lint:fix && npm run lint",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjsicam/node-graphql-microservices.git"
  },
  "keywords": [
    "graphql",
    "microservices",
    "grpc"
  ],
  "author": "Benj Sicam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjsicam/node-graphql-microservices/issues"
  },
  "homepage": "https://github.com/benjsicam/node-graphql-microservices#readme",
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@grpc/proto-loader": "^0.5.2",
    "aigle": "^1.14.1",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.2.1",
    "glob": "^7.1.4",
    "graphql-redis-subscriptions": "^2.1.1",
    "graphql-scalars": "^1.0.1",
    "graphql-subscriptions": "^1.1.0",
    "graphql-yoga": "^1.18.3",
    "grpc": "^1.23.3",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "pino": "^5.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cpy-cli": "^2.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.2",
    "pino-pretty": "^3.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
}
