{
  "name": "node-graphql-microservices",
  "version": "1.0.0",
  "description": "A sample GraphQL API built in Node.js with gRPC back-end microservices. Used for learning/trial purposes only.",
  "scripts": {
    "start": "nodemon --exec babel-node ./src/main.js",
    "test": "jest --watch",
    "lint": "eslint --max-warnings=0 ./src",
    "lint:fix": "eslint --fix ./src",
    "precommit": "npm run lint:fix && npm run lint",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjsicam/node-graphql-microservices.git"
  },
  "keywords": [
    "graphql",
    "microservices",
    "grpc"
  ],
  "author": "Benj Sicam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjsicam/node-graphql-microservices/issues"
  },
  "homepage": "https://github.com/benjsicam/node-graphql-microservices#readme",
  "dependencies": {
    "@grpc/grpc-js": "^0.5.2",
    "@grpc/proto-loader": "^0.5.1",
    "aigle": "^1.14.1",
    "graphql-redis-subscriptions": "^2.1.0",
    "graphql-yoga": "^1.18.1",
    "ioredis": "^4.11.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.14",
    "pino": "^5.12.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/runtime": "^7.5.4",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-plugin-inline-dotenv": "^1.3.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-airbnb": "^4.0.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.8.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.1",
    "pino-pretty": "^3.2.0",
    "prettier": "^1.18.2"
  }
}
